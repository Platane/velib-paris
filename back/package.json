        "babel-register": "6.26.0",
        "flow-bin": "0.60.1",
        "flow-typed": "2.2.3",
        "jest": "22.0.0"
    },
    "engines": {
        "node": "6.11.x"
    },
    "scripts": {
        "flow-typed": "flow-typed install",
        "publishAll": "node -e \"require('babel-register');require('./src/module/publishAllBatch/cmd.js')\"",
        "build":
