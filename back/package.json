        "test-skip-io": "jest '^((?!read\\/).)*.__tests__\\/.*$'",
        "test": "jest"
    },
    "jest": {
        "verbose": true,
        "collectCoverage": true,
        "modulePathIgnorePatterns": [".*/lib/.*"]
    }
